{
  "size": "MICRO",
  "deploymentStrategy": "RollingUpdate",
  "lbType": "internal",
  "elbIdleTimeoutSeconds": 300,
  "secretFileMounts": [],
  "protocolGroup": "tcp",
  "scaling": {
    "hpaEnabled": true,
    "hpaMinReplicas": 1,
    "hpaMaxReplicas": 1,
    "hpaMetricThreshold": 60
  },
  "liveness": {
    "enableLivenessTCP": true,
    "livenessPort": 3000,
    "livenessInitialDelay": 30,
    "livenessTimeout": 10,
    "livenessFailureThreshold": 20,
    "livenessPeriod": 10,
    "livenessSuccessThreshold": 1
  },
  "readiness": {
    "enableReadinessTCP": true,
    "readinessInitialDelay": 30,
    "readinessFailureThreshold": 20,
    "readinessSuccessThreshold": 1,
    "readinessPort": 3000,
    "readinessTimeout": 10,
    "readinessPeriod": 10
  },
  "ports": [
    {
      "name": "http",
      "containerPort": 3000,
      "lbPort": 3000
    }
  ],
  "build": {
    "ci": "external_image",
    "external_image": "486456986266.dkr.ecr.us-west-1.amazonaws.com/ops/demo-backend:a3c3882-2"
  },
  "credentialRequests": {
    "dbs": {
      "mysql": [],
      "mongo": [
        {
          "resourceType": "mongo",
          "resourceName": "demomongo",
          "permission": "READ_WRITE",
          "environmentVariables": [
            {
              "userName": "MONGODB_USER",
              "password": "MONGODB_PASSWORD"
            }
          ]
        }
      ]
    },
    "queues": {
      "rabbitmq": []
    },
    "cloud": []
  },
  "environmentVariables": {
    "static": {
      "NODE_ENV": "prod"
    },
    "dynamic": {
    }
  },
  "k8s_service_names": [],
  "ingress_rules": [
  ],
  "resourceAllocationStrategy": "GENERAL_PURPOSE",
  "command": ["/bin/bash", "-c", "MONGODB_URI=mongodb://${MONGODB_USER}:${MONGODB_PASSWORD}@mongo-rs-demomongo-mongodb:27017/?authSource=admin npm start dev"]
}
