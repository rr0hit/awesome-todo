{
  "size": "MICRO",
  "deploymentStrategy": "RollingUpdate",
  "lbType": "internal",
  "elbIdleTimeoutSeconds": 300,
  "secretFileMounts": [],
  "protocolGroup": "tcp",
  "scaling": {
    "hpaEnabled": true,
    "hpaMinReplicas": 1,
    "hpaMaxReplicas": 1,
    "hpaMetricThreshold": 60
  },
  "liveness": {
    "enableLivenessTCP": true,
    "livenessPort": 3000,
    "livenessInitialDelay": 30,
    "livenessTimeout": 10,
    "livenessFailureThreshold": 20,
    "livenessPeriod": 10,
    "livenessSuccessThreshold": 1
  },
  "readiness": {
    "enableReadinessTCP": true,
    "readinessInitialDelay": 30,
    "readinessFailureThreshold": 20,
    "readinessSuccessThreshold": 1,
    "readinessPort": 3000,
    "readinessTimeout": 10,
    "readinessPeriod": 10
  },
  "ports": [
    {
      "name": "http",
      "containerPort": 3000,
      "lbPort": 3000
    }
  ],
  "build": {
    "ci": "external_image",
    "external_image": "486456986266.dkr.ecr.us-west-1.amazonaws.com/ops/demo-frontend:a3c3882-2"
  },
  "credentialRequests": {
    "dbs": {
      "mysql": [],
      "mongo": []
    },
    "queues": {
      "rabbitmq": []
    },
    "cloud": []
  },
  "environmentVariables": {
    "static": {
      "CI": "true",
      "DANGEROUSLY_DISABLE_HOST_CHECK": "true"
    },
    "dynamic": {
    }
  },
  "k8s_service_names": [],
  "ingress_rules": [
    {
      "ingress": "demoingress",
      "path": "/",
      "targetPort": 3000,
      "domainPrefix": ""
    }
  ],
  "resourceAllocationStrategy": "GENERAL_PURPOSE"
}
